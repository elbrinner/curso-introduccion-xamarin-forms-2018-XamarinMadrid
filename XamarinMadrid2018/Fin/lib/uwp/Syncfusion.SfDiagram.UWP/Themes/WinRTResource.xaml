                                 Style="{Binding DataContext.ShapeStyle,RelativeSource={RelativeSource TemplatedParent}}" 
                                   Stretch="Fill" x:Name="BackgroundGlyph"/>
                            <ContentPresenter 
                                 Content="{Binding DataContext.Content,RelativeSource={RelativeSource TemplatedParent}}" 
                                 ContentTemplate="{Binding DataContext.ContentTemplate,RelativeSource={RelativeSource TemplatedParent}}"  
                                 x:Name="Content" RenderTransformOrigin="0.5,0.5" Tag="{TemplateBinding Foreground}">
                            </ContentPresenter>
                        </Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="PointerOver">
                                    <Storyboard>
                                        <ColorAnimation To="#595959"
                            Storyboard.TargetName="BackgroundGlyph"
                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ColorAnimation To="#5d9692"
                            Storyboard.TargetName="BackgroundGlyph"
                            Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)"/>
                                        <ColorAnimation To="Black"
                            Storyboard.TargetName="Content"
                            Storyboard.TargetProperty="(ContentPresenter.Tag).(SolidColorBrush.Color)"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <Style TargetType="local:Selector">
        <Setter Property="TooltipTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid Background="{StaticResource SelectorGridBackgroundBrush}" Margin="-90" HorizontalAlignment="Center" VerticalAlignment="Top">
                        <Border CornerRadius="10" Background="{StaticResource SelectorBorderBackgroundBrush}" BorderThickness="1" BorderBrush="#DDB654" Opacity="0.8">
                            <TextBlock Margin="10" Text="{Binding TooltipContent}" Foreground="{StaticResource SelectorTextblockForegroundBrush}" FontSize="14"/>
                        </Border>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:Selector">
                    <Border
    Background="{TemplateBinding Background}"
    BorderBrush="{TemplateBinding BorderBrush}"
    BorderThickness="{TemplateBinding BorderThickness}">
                        <panels:SelectorPanel x:Name="PART_SelectorPanel">
                            <panels:SelectorPanel.Resources>
                                <Style x:Key="pathStyle" TargetType="Shape">
                                    <Setter Property="StrokeThickness" Value="1.5"/>
                                    <Setter Property="Stroke" Value="{StaticResource PathStrokeThemeBrush}"/>
                                </Style>
                            </panels:SelectorPanel.Resources>
                            <Grid Opacity="0.8" Visibility="{TemplateBinding TooltipVisibility}"  Background="Transparent">
                                <ContentPresenter Content="{Binding}" ContentTemplate="{TemplateBinding TooltipTemplate}"/>
                            </Grid>
                            <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                            <Rectangle x:Name="Part_Rectangle" Style="{StaticResource pathStyle}" Stretch="Fill" StrokeDashArray="2, 2"></Rectangle>
                            <Line X1="0" X2="0" Y1="0" Y2="0" Visibility="{Binding SelectorConstraints, Converter={StaticResource NulltoVisibilityConverter},ConverterParameter=Rotator}" StrokeDashArray="2, 2" Style="{StaticResource pathStyle}"/>
                            <localControls:DiagramThumb x:Name="PART_TopLeft" HorizontalAlignment="Left" ControlPointType="TopLeftResizer" VerticalAlignment="Top" Visibility="{Binding SelectorConstraints, Converter={StaticResource NulltoVisibilityConverter},ConverterParameter=Resize}"/>
                            <localControls:DiagramThumb x:Name="PART_Top" HorizontalAlignment="Center" ControlPointType="TopResizer" VerticalAlignment="Top" Visibility="{Binding SelectorConstraints, Converter={StaticResource NulltoVisibilityConverter},ConverterParameter=Resize}"/>
                            <localControls:DiagramThumb x:Name="PART_TopRight" HorizontalAlignment="Right" ControlPointType="TopRightResizer" VerticalAlignment="Top" Visibility="{Binding SelectorConstraints, Converter={StaticResource NulltoVisibilityConverter},ConverterParameter=Resize}"/>
                            <localControls:DiagramThumb x:Name="PART_Left" HorizontalAlignment="Left" ControlPointType="LeftResizer" VerticalAlignment="Center" Visibility="{Binding SelectorConstraints, Converter={StaticResource NulltoVisibilityConverter},ConverterParameter=Resize}" />
                            <localControls:DiagramThumb x:Name="PART_Right" HorizontalAlignment="Right" ControlPointType="RightResizer" VerticalAlignment="Center" Visibility="{Binding SelectorConstraints, Converter={StaticResource NulltoVisibilityConverter},ConverterParameter=Resize}"/>
                            <localControls:DiagramThumb x:Name="PART_BottomLeft" HorizontalAlignment="Left" ControlPointType="BottomLeftResizer" VerticalAlignment="Bottom" Visibility="{Binding SelectorConstraints, Converter={StaticResource NulltoVisibilityConverter},ConverterParameter=Resize}"/>
                            <localControls:DiagramThumb x:Name="PART_Bottom" HorizontalAlignment="Center" ControlPointType="BottomResizer" VerticalAlignment="Bottom" Visibility="{Binding SelectorConstraints, Converter={StaticResource NulltoVisibilityConverter},ConverterParameter=Resize}"/>
                            <localControls:DiagramThumb x:Name="PART_BottomRight" HorizontalAlignment="Right" ControlPointType="BottomRightResizer" VerticalAlignment="Bottom" Visibility="{Binding SelectorConstraints, Converter={StaticResource NulltoVisibilityConverter},ConverterParameter=Resize}"/>
                            <localControls:DiagramThumb Style="{StaticResource PivotThumbStyle}" x:Name="PART_Pivot" ControlPointType="Pivot" Visibility="Collapsed"/>
                            <localControls:DiagramThumb Style="{StaticResource RotatorThumbStyle}" x:Name="PART_Rotator" ControlPointType="Rotator" Visibility="{Binding SelectorConstraints, Converter={StaticResource NulltoVisibilityConverter},ConverterParameter=Rotator}" />
                        </panels:SelectorPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>

