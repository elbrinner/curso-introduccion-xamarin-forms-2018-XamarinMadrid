<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
      xmlns:local="using:Syncfusion.UI.Xaml.Diagram"
    xmlns:localControls="using:Syncfusion.UI.Xaml.Diagram.Controls"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:stencil="using:Syncfusion.UI.Xaml.Diagram.Stencil"    
    xmlns:util="using:Syncfusion.UI.Xaml.Diagram.Utility"
    xmlns:printing="using:Syncfusion.UI.Xaml.Diagram.Controls"
    xmlns:panels="using:Syncfusion.UI.Xaml.Diagram.Panels">
    
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="PathStrokeThemeBrush" Color="#FFFFFF" />
            <x:Double x:Key="ConnectorThemeThickness">1</x:Double>
            <x:Double x:Key="ConnectorDecoratorThemeWidth">10</x:Double>
            <x:Double x:Key="ConnectorDecoratorThemeHeight">10</x:Double>
            <x:Double x:Key="PortThemeWidth">10</x:Double>
            <x:Double x:Key="PortThemeHeight">10</x:Double>
            <SolidColorBrush x:Key="RulerBackgroundThemeBrush" Color="#4F4F4F" />
            <SolidColorBrush x:Key="RulerForegroundThemeBrush" Color="#9B9B9B" />
            <SolidColorBrush x:Key="SfDiargamBackgroundThemeBrush" Color="#202020"/>       
            <SolidColorBrush x:Key="OrthoThumbThemeBrush" Color="#3a3a3a"></SolidColorBrush>
            <SolidColorBrush x:Key="BezierThumbThemeBrush" Color="Black"></SolidColorBrush>
            <!--Stencil-->
            <SolidColorBrush x:Key="SymbolPressedBackgroundThemeBrush" Color="#C4C2C2"/>
            <SolidColorBrush x:Key="SymbolGroupBackgroundThemeBrush" Color="#2D2D2D"></SolidColorBrush>
            <SolidColorBrush x:Key="SymbolGroupHeaderBackgroundThemeBrush" Color="#3A3A3A"></SolidColorBrush>
            <SolidColorBrush x:Key="SymbolGroupBorderThemeBrush" Color="#808080"></SolidColorBrush>
            <SolidColorBrush x:Key="SymbolGroupBackgroundExpandThemeBrush" Color="#595959"></SolidColorBrush>
            <SolidColorBrush x:Key="ExpanderPathFillThemeBrush" Color="#CCCCCC"></SolidColorBrush>
            <SolidColorBrush x:Key="SymbolGroupHeaderForgroundThemeBrush" Color="#EDEEEF"></SolidColorBrush>

        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="PathStrokeThemeBrush" Color="#FF777E84" />
            <x:Double x:Key="ConnectorThemeThickness">1</x:Double>
            <x:Double x:Key="ConnectorDecoratorThemeWidth">10</x:Double>
            <x:Double x:Key="ConnectorDecoratorThemeHeight">10</x:Double>
            <x:Double x:Key="PortThemeWidth">10</x:Double>
            <x:Double x:Key="PortThemeHeight">10</x:Double>
            <SolidColorBrush x:Key="RulerBackgroundThemeBrush" Color="#F4F4F4" />
            <SolidColorBrush x:Key="RulerForegroundThemeBrush" Color="#919191" />
            <SolidColorBrush x:Key="SfDiargamBackgroundThemeBrush" Color="#ECECEC"/>
            <SolidColorBrush x:Key="OrthoThumbThemeBrush" Color="#3a3a3a"></SolidColorBrush>
            <SolidColorBrush x:Key="BezierThumbThemeBrush" Color="#0D4423"></SolidColorBrush>
            <!--Stencil-->
            <SolidColorBrush x:Key="SymbolPressedBackgroundThemeBrush" Color="#C4C2C2"/>
            <SolidColorBrush x:Key="SymbolGroupBackgroundThemeBrush" Color="#F4F4F4"></SolidColorBrush>
            <SolidColorBrush x:Key="SymbolGroupHeaderBackgroundThemeBrush" Color="#EAEAEA"></SolidColorBrush>
            <SolidColorBrush x:Key="SymbolGroupBorderThemeBrush" Color="#c6c6c6"></SolidColorBrush>
            <SolidColorBrush x:Key="SymbolGroupBackgroundExpandThemeBrush" Color="#D3D3D3"></SolidColorBrush>
            <SolidColorBrush x:Key="ExpanderPathFillThemeBrush" Color="#6D6D6D"></SolidColorBrush>
            <SolidColorBrush x:Key="SymbolGroupHeaderForgroundThemeBrush" Color="#2D2D2D"></SolidColorBrush>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    <SolidColorBrush x:Key="SelectorThumbBrush" Color="White"/>

    <x:Double x:Key="RotatorEllipseStrokeThickness">1.5</x:Double>
    <x:Double x:Key="OuterPivotThumbStrokeThickness">8</x:Double>
    <x:Double x:Key="PivotThumbStrokeThickness">1</x:Double>
    <Thickness x:Key="SymbolFilterBorderThickness">1</Thickness>
    <Thickness x:Key="SymbolBorderThickness">1</Thickness>
    
    <!--<ScrollViewer>-->
    <SolidColorBrush x:Key="ScrollViewerBorderBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="ScrollViewerBackgroundBrush" Color="Transparent"/>
    <Thickness x:Key="ScrollViewerBorderThickness">0</Thickness>
    <!--<DiagramThumb>-->
    <SolidColorBrush x:Key="BezierthumbFillBrush" Color="White"/>
    <x:Double x:Key="BezierthumbStrokeThickness">1</x:Double>
    <SolidColorBrush x:Key="BezierthumbEllipseFillBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="OrthogonalThumbFillBrush" Color="#e2e2e2"/>
    <SolidColorBrush x:Key="OrthogonalThumbBackgroundBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="EndthumbFillBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="EndthumbStrokeBrush" Color="#4288DB"/>
    <x:Double x:Key="EndthumbStrokeThickness">1.1</x:Double>
    <SolidColorBrush x:Key="DiagramThumbFillBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="DiagramThumbStrokeBrush" Color="Black"/>
    <x:Double x:Key="DiagramThumbStrokeThickness">1.1</x:Double>
    <!--<OverView>-->
    <SolidColorBrush x:Key="OverviewUnFocusBrush" Color="#22000000"/>
    <SolidColorBrush x:Key="OverviewBackgroundBrush" Color="White"/>
    <SolidColorBrush x:Key="OverviewResizerBackgroundBrush" Color="Red"/>
    <Thickness x:Key="OverviewResizerBorderThickness">0</Thickness>
    <!--<Printing>-->


    <!--<Selector>-->
    <SolidColorBrush x:Key="SelectorGridBackgroundBrush" Color="Transparent" />
    <SolidColorBrush x:Key="SelectorBorderBackgroundBrush" Color="#FFF7EEBC" />
    <SolidColorBrush x:Key="SelectorTextblockForegroundBrush" Color="Black" />
    <SolidColorBrush x:Key="SelectorQuickDeleteCommandBackgroundThumbBrush" Color="#FFD74651"/>
    <SolidColorBrush x:Key="SelectorQuickDrawCommandBackgroundThumbBrush" Color="#FF60437B"/>
    <SolidColorBrush x:Key="SelectorQuickDuplicateCommandBackgroundThumbBrush" Color="#FF0D72BA"/>
    <!--<Selector>-->
    <SolidColorBrush x:Key="SelectorThumbPivotBrush" Color="White"/>
    <SolidColorBrush x:Key="SelectorThumbPivotStrokeBrush" Color="Black"/>
    <SolidColorBrush x:Key="SelectorQuickCommandForegroundThumbBrush" Color="White"/>

    <!--<Rotator>-->
    <SolidColorBrush x:Key="RotatorEllipseFillBrush" Color="#333333" />
    <SolidColorBrush x:Key="RotatorEllipseStrokeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="RotatorThumbFillBrush" Color="White" />
    <!--<Pivot>-->
    <SolidColorBrush x:Key="OuterPivotThumbFillBrush" Color="#4288DB" />
    <SolidColorBrush x:Key="OuterPivotThumbStrokeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="PivotThumbFillBrush" Color="White" />
